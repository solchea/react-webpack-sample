{
  "name": "react-webpack-sample",
  "version": "0.0.1",
  "description": "A sample app with react, webpack, react-router",
  "main": "index.js",
  "scripts": {
    "start-keep": "npm run build && node ./bin/www | garnish",
    "start": "node ./bin/www | garnish",
    "dev": "NODE_ENV=development webpack-dev-server --hot --inline --progress --colors --config webpack.dev.config.js",
    "build": "rm -rf public/build; webpack --config webpack.prod.config.js --progress --colors",
    "test": "standard --verbose",
    "standard": "standard"
  },
  "author": "sol",
  "license": "ISC",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-trailing-function-commas": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.23.0",
    "debug": "^2.2.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
    "garnish": "^5.1.1",
    "glob": "^7.0.3",
    "hbs": "^4.0.0",
    "history": "^2.0.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "less-middleware": "^2.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^4.8.2",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "react": "^0.14.8",
    "react-document-title": "^2.0.1",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "serve-favicon": "^2.3.0",
