{
  "name": "react-webpack-sample",
  "version": "0.0.1",
  "description": "A sample app with react, webpack, react-router",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./bin/www | garnish",
    "dev": "NODE_ENV=development webpack-dev-server --hot --inline --progress --colors --config webpack.dev.config.js",
    "build": "rm -rf public/build; webpack --config webpack.prod.config.js --progress --colors",
    "test": "standard --verbose",
    "standard": "standard"
  },
  "author": "sol",
  "license": "ISC",
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "body-parser": "^1.14.1",
    "cookie-parser": "^1.4.0",
    "css-loader": "^0.23.0",
    "debug": "^2.2.0",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "fastclick": "^1.0.6",
    "garnish": "^5.0.0",
    "glob": "^6.0.1",
    "hbs": "^4.0.0",
    "history": "^1.13.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "less-middleware": "^2.0.1",
    "less-plugin-npm-import": "^2.1.0",
    "lodash": "^3.10.1",
    "morgan": "^1.6.1",
    "path": "^0.12.7",
    "react": "^0.14.3",
    "react-document-title": "^2.0.1",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.6.1",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "webpack-dev-server": "^1.14.0"
  }
}
