<!DOCTYPE html>
<html>
  <head>
    <title>{{title}}</title>
    {{#each css}}
    <link href="{{this}}" rel="stylesheet" type="text/css">
    {{/each}}
    <script>window.__INITIAL_STATE__ = {{{initialState}}};</script>
  </head>
  <body>
    {{{body}}}
    {{#each scripts}}
    <script src="{{this}}"></script>
	{{/each}}
  </body>
</html>
